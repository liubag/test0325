<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="Struts2_grade" extends="struts-default"> 						
		<action name="showGrade_by_page" class="com.aspsystem.action.GradeAction" method="showGrade_by_page">
			<result name="success">/student/selScore.jsp</result>
			<result name="input">/public/error.jsp</result>
		</action>
		
		<action name="studentRank" class="com.aspsystem.action.GradeAction" method="studentRank">
			<result name="success">/teacher/teacher_studentRank.jsp</result>
			<result name="input">/public/error.jsp</result>
		</action>
	</package> 
</struts>
